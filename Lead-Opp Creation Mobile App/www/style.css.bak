/* toggle css */
.onoffswitch
{
    float: left;
    margin: 0 15px 0 0;
    position: relative; width: 75px;
    -webkit-user-select:none; -moz-user-select:none; -ms-user-select: none;
}

.onoffswitch1{
    float: right;
    margin: 0;
    position: relative;
    width: 75px;
    -webkit-user-select: none;
}

.onoffswitch-checkbox,
.onoffswitch-checkbox1 {
    display: none;
}
.onoffswitch-label,
.onoffswitch-label1 {
    display: block;
    overflow: hidden;
    cursor: pointer;
    border: 2px solid #265599;
    border-radius: 14px;
}
.onoffswitch-inner,
.onoffswitch-inner1 {
    display: block;
    width: 200%;
    margin-left: -100%;
    transition: margin 0.3s ease-in 0s;
}
.onoffswitch-inner:before, .onoffswitch-inner:after,
.onoffswitch-inner1:before, .onoffswitch-inner1:after {
    display: block; float: left; width: 50%; height: 21px; padding: 0; line-height: 22px;
    font-size: 15px; color: white; font-family: Trebuchet, Arial, sans-serif; font-weight: bold;
    box-sizing: border-box;
}
.onoffswitch-inner:before,
.onoffswitch-inner1:before {
    padding-right: 15px;
    background-color: #FFFFFF; color:#265599 ;
    text-align: center;
}

.onoffswitch-inner:before { content: "Org"; }

.onoffswitch-inner1:before { content: "Used"; }

.onoffswitch-inner:after,
.onoffswitch-inner1:after {
    padding-left: 15px;
    background-color: #fff;
    color: #265599;
    text-align: center;
}

.onoffswitch-inner:after { content: "Ind"; }

.onoffswitch-inner1:after { content: "New"; }

.onoffswitch-switch,
.onoffswitch-switch1 {
    display: block; width: 26px; margin: 0px;
    background: #FFFFFF;
    position: absolute; top: 0; bottom: 0;
    right: 49px;
    border: 2px solid #265599; border-radius: 14px;
    transition: all 0.3s ease-in 0s; 
}
.onoffswitch-checkbox:checked + .onoffswitch-label .onoffswitch-inner,
.onoffswitch-checkbox1:checked + .onoffswitch-label1 .onoffswitch-inner1 {
    margin-left: 0;
}
.onoffswitch-checkbox:checked + .onoffswitch-label .onoffswitch-switch,
.onoffswitch-checkbox1:checked + .onoffswitch-label1 .onoffswitch-switch1 {
    right: 0px; 
}

